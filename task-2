     <script>
        let human='';
        fetch('https://www.coursehubiitg.in/api/codingweek/contributions')
            .then(num => num.json())
            .then(human1 => {
                function sorting(hi1,hi2) {
                    return hi2.points - hi1.points;
                }
                human = human1.sort(sorting)
                console.log(human)
          
           document.querySelector('.moha1').innerText = `${human[0].name} - ${human[0].points}`;
                document.querySelector('.moha2').innerText = `${human[1].name} - ${human[1].points}`;                
                document.querySelector('.moha3').innerText = `${human[2].name} - ${human[2].points}`;                
                document.querySelector('.na1').innerHTML = `<img src="${human[0].avatar}">`;  
                document.querySelector('.na2').innerHTML = `<img src="${human[1].avatar}">`;                
                document.querySelector('.na3').innerHTML = `<img src="${human[2].avatar}">`;
          
           document.querySelector('.nam1').innerText = `${human[3].name}`; 
                document.querySelector('.poi1').innerText = `${human[3].points}`;
                document.querySelector('.sa1').innerHTML = `<img src="${human[3].avatar}">`;  
                
                document.querySelector('.nam2').innerText = `${human[4].name}`; 
                document.querySelector('.poi2').innerText = `${human[4].points}`;
                document.querySelector('.sa2').innerHTML = `<img src="${human[4].avatar}">`;  

                document.querySelector('.nam3').innerText = `${human[5].name}`; 
                document.querySelector('.poi3').innerText = `${human[5].points}`;
                document.querySelector('.sa3').innerHTML = `<img src="${human[5].avatar}">`;  

                document.querySelector('.nam4').innerText = `${human[6].name}`; 
                document.querySelector('.poi4').innerText = `${human[6].points}`;
                document.querySelector('.sa4').innerHTML = `<img src="${human[6].avatar}">`;  

                document.querySelector('.nam5').innerText = `${human[7].name}`; 
                document.querySelector('.poi5').innerText = `${human[7].points}`;
                document.querySelector('.sa5').innerHTML = `<img src="${human[7].avatar}">`;  

                document.querySelector('.nam6').innerText = `${human[8].name}`; 
                document.querySelector('.poi6').innerText = `${human[8].points}`;
                document.querySelector('.sa6').innerHTML = `<img src="${human[8].avatar}">`;  
        })
        </script>
